{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "paths": {
            "@httptoolkit/proxy-agent": ["../custom-typings/proxy-agent.d.ts"]
        }
    },
    "compileOnSave": true,
    "buildOnSave": true,
    "include": [
        "../custom-typings/*.d.ts",
        "**/*.ts",
    ],
    "filesGlob": [
        "../custom-typings/*.d.ts",
        "**/*.ts",
    ]
}
